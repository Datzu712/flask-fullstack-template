{
  "name": "flask-fullstack-template",
  "version": "1.0.0",
  "description": "Flask FullStack template",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --mode=production --node-env=production",
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --mode=development",
    "build:prod": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --mode=production --node-env=production",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --watch",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Datzu712/flask-fullstack-template.git"
  },
  "keywords": [
    "flask",
    "docker",
    "compose",
    "mysql",
    "webpack",
    "nginx",
    "redis",
    "JWT",
    "MVC",
    "LAMP",
    "sqlalchemy",
    "typescript",
    "bootstrap",
    "datatables",
    "mdb-ui-kit",
    "moment",
    "newrelic"
  ],
  "author": "Datzu712",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Datzu712/flask-fullstack-template/issues"
  },
  "homepage": "https://github.com/Datzu712/flask-fullstack-template#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@types/bootstrap": "^5.2.10",
    "@types/jquery": "^3.5.30",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "5.5.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "cross-env": "^7.0.3",
    "datatables.net": "^2.3.2",
    "datatables.net-bs5": "^2.1.7",
    "datatables.net-responsive-bs5": "^3.0.3",
    "jquery": "^3.7.1",
    "mdb-ui-kit": "^8.0.0",
    "moment": "^2.30.1",
    "style-loader": "^4.0.0",
    "sucrase": "^3.35.0",
    "ts-loader": "^9.5.1",
    "uuid": "^10.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  }
}
